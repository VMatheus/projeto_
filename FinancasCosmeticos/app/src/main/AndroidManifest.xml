<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tecnoia.matheus.financascosmeticos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.DadosOffline"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_01"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_01"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ContainerActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".revendedor.EditarPerfilRevendedorActivity"
            android:label="@string/title_activity_editar_perfil_revendedor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PerfilActivity"
            android:label="@string/title_activity_perfil"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>